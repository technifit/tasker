{
  "name": "@technifit/employer",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "clean": "rm -rf .turbo node_modules .cache build types/remix-routes.d.ts",
    "dev": "remix vite:dev",
    "format:fix": "prettier . --write --cache --cache-location='node_modules/.cache/.prettiercache' --log-level=warn",
    "format": "prettier . --check --cache --cache-location='node_modules/.cache/.prettiercache'",
    "lint:fix": "eslint . --fix --cache --cache-location 'node_modules/.cache/.eslintcache' --max-warnings 0",
    "lint": "eslint . --cache --cache-location 'node_modules/.cache/.eslintcache' --max-warnings 0",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@clerk/remix": "3.2.4",
    "@remix-run/react": "2.8.1",
    "@remix-run/serve": "2.8.1",
    "@sentry/integrations": "7.111.0",
    "@sentry/profiling-node": "7.111.0",
    "@sentry/remix": "7.111.0",
    "@technifit/email": "*",
    "@technifit/ui": "*",
    "@vercel/analytics": "1.2.2",
    "@vercel/remix": "2.8.1",
    "@vercel/speed-insights": "1.0.10",
    "isbot": "5.1.4",
    "lodash": "4.17.21",
    "lucide-react": "0.371.0",
    "nanoid": "5.0.7",
    "pretty-cache-header": "1.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remix-hook-form": "4.3.1",
    "remix-params-helper": "0.5.1",
    "remix-routes": "1.7.3",
    "remix-themes": "1.3.1",
    "remix-utils": "7.6.0",
    "zod": "3.22.5"
  },
  "devDependencies": {
    "@remix-run/dev": "2.8.1",
    "@sentry/vite-plugin": "2.16.1",
    "@technifit/eslint-config": "*",
    "@technifit/tailwind-config": "*",
    "@technifit/tsconfig": "*",
    "@types/cookie": "0.6.0",
    "@types/lodash": "4.17.0",
    "@types/react": "18.2.79",
    "@types/react-dom": "18.2.25",
    "cookie": "0.6.0",
    "eslint": "8.57.0",
    "npm-run-all2": "6.1.2",
    "postcss": "8.4.38",
    "remix-development-tools": "4.1.4",
    "remix-flat-routes": "0.6.4",
    "tailwindcss": "3.4.3",
    "tailwindcss-animate": "1.0.7",
    "typescript": "5.4.5",
    "typescript-remix-routes-plugin": "1.0.1",
    "vite": "5.2.10",
    "vite-tsconfig-paths": "4.3.2"
  },
  "packageManager": "pnpm@8.15.7",
  "engines": {
    "node": ">=20.12.2"
  }
}
