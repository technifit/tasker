{
  "name": "@technifit/employer",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:routes": "remix-routes -o ./types",
    "build:remix": "remix build --sourcemap",
    "dev": "run-p dev:*",
    "dev:routes": "remix-routes -w ./types",
    "dev:remix": "remix dev",
    "clean": "rm -rf .turbo node_modules .cache build types/remix-routes.d.ts",
    "lint": "SKIP_ENV_VALIDATION=1  eslint .",
    "lint:fix": "pnpm lint --fix",
    "start": "remix-serve build",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@clerk/remix": "2.10.2",
    "@remix-run/react": "1.19.3",
    "@remix-run/serve": "1.19.3",
    "@sentry/integrations": "7.69.0",
    "@sentry/profiling-node": "1.2.1",
    "@sentry/remix": "7.69.0",
    "@technifit/ui": "0.1.0",
    "@vercel/analytics": "1.0.2",
    "@vercel/remix": "1.19.3",
    "isbot": "3.6.13",
    "lodash": "4.17.21",
    "nanoid": "4.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remix-hook-form": "2.0.0",
    "remix-params-helper": "0.4.10",
    "remix-routes": "1.4.4",
    "remix-themes": "1.2.1",
    "remix-utils": "6.6.0",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@remix-run/dev": "1.19.3",
