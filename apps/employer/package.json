{
  "name": "@technifit/employer",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build:remix": "remix build --sourcemap",
    "build:routes": "remix-routes -o ./types",
    "build": "run-s build:*",
    "clean": "rm -rf .turbo node_modules .cache build types/remix-routes.d.ts",
    "dev:remix": "remix dev",
    "dev:routes": "remix-routes -w ./types",
    "dev": "run-p dev:*",
    "format:fix": "prettier . --write --cache --cache-location='node_modules/.cache/.prettiercache' --log-level=warn",
    "format": "prettier . --check --cache --cache-location='node_modules/.cache/.prettiercache'",
    "start": "remix-serve build",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@clerk/remix": "3.1.12",
    "@remix-run/react": "2.4.0",
    "@remix-run/serve": "2.4.0",
    "@sentry/integrations": "7.90.0",
    "@sentry/profiling-node": "1.3.2",
    "@sentry/remix": "7.90.0",
    "@technifit/ui": "1.0.0",
    "@vercel/analytics": "1.1.1",
    "@vercel/remix": "2.4.1",
    "isbot": "3.7.1",
    "lodash": "4.17.21",
    "lucide-react": "0.299.0",
    "nanoid": "5.0.4",
    "pretty-cache-header": "1.0.0",
    "react-dom": "18.2.0",
    "react": "18.2.0",
    "remix-hook-form": "3.2.3",
    "remix-params-helper": "0.5.1",
    "remix-routes": "1.5.0",
    "remix-themes": "1.2.2",
    "remix-utils": "7.4.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@remix-run/dev": "2.4.0",
    "@technifit/tailwind-config": "0.1.0",
    "@technifit/tsconfig": "1.0.0",
    "@types/cookie": "0.6.0",
    "@types/lodash": "4.14.202",
    "@types/react-dom": "18.2.18",
    "@types/react": "18.2.45",
    "cookie": "0.6.0",
    "npm-run-all": "4.1.5",
    "remix-flat-routes": "0.6.4",
    "tailwindcss-animate": "1.0.7",
    "tailwindcss": "3.4.0",
    "typescript-remix-routes-plugin": "1.0.1",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": ">=20.10.0"
  }
}
