{
  "name": "@technifit/employer",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build:remix": "remix build --sourcemap",
    "build:routes": "remix-routes -o ./types",
    "build": "run-s build:*",
    "clean": "rm -rf .turbo node_modules .cache build types/remix-routes.d.ts",
    "dev:remix": "remix dev",
    "dev:routes": "remix-routes -w ./types",
    "dev": "run-p dev:*",
    "format:fix": "prettier . --write --cache --cache-location='node_modules/.cache/.prettiercache' --loglevel=warn",
    "format": "prettier . --check --cache --cache-location='node_modules/.cache/.prettiercache'",
    "lint": "eslint . --cache --cache-location 'node_modules/.cache/.eslintcache' --max-warnings 0",
    "lint:fix": "eslint . --fix --cache --cache-location 'node_modules/.cache/.eslintcache' --max-warnings 0",
    "start": "remix-serve build",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@clerk/remix": "2.10.3",
    "@remix-run/react": "1.19.3",
    "@remix-run/serve": "1.19.3",
    "@sentry/integrations": "7.70.0",
    "@sentry/profiling-node": "1.2.1",
    "@sentry/remix": "7.70.0",
    "@technifit/ui": "0.1.0",
    "@vercel/analytics": "1.0.2",
    "@vercel/remix": "1.19.3",
    "isbot": "3.7.0",
    "lodash": "4.17.21",
    "nanoid": "5.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remix-hook-form": "2.0.1",
    "remix-params-helper": "0.4.10",
    "remix-routes": "1.4.5",
    "remix-themes": "1.2.1",
    "remix-utils": "6.6.0",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@remix-run/dev": "1.19.3",
    "@remix-run/eslint-config": "1.19.3",
    "@technifit/eslint-config": "0.2.0",
    "@typescript-eslint/eslint-plugin": "6.7.2",
    "@typescript-eslint/parser": "6.7.2",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-turbo": "1.10.14",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react": "7.33.2",
    "@technifit/tailwind-config": "0.1.0",
    "@types/cookie": "0.5.2",
    "@types/lodash": "4.14.198",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "cookie": "0.5.0",
    "eslint": "8.50.0",
    "npm-run-all": "4.1.5",
    "remix-flat-routes": "0.5.11",
    "tailwindcss": "3.3.3",
    "tailwindcss-animate": "1.0.7",
    "typescript": "5.2.2",
    "typescript-remix-routes-plugin": "1.0.1"
  },
  "engines": {
    "node": ">=18.18.0"
  }
}
