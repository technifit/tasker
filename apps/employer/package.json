{
  "name": "@technifit/employer",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build:remix": "remix build --sourcemap",
    "build:routes": "remix-routes -o ./types",
    "build": "run-s build:*",
    "clean": "rm -rf .turbo node_modules .cache build types/remix-routes.d.ts",
    "dev:remix": "remix dev",
    "dev:routes": "remix-routes -w ./types",
    "dev": "run-p dev:*",
    "format:fix": "prettier . --write --cache --cache-location='node_modules/.cache/.prettiercache' --log-level=warn",
    "format": "prettier . --check --cache --cache-location='node_modules/.cache/.prettiercache'",
    "lint:fix": "eslint . --fix --cache --cache-location 'node_modules/.cache/.eslintcache' --max-warnings 0",
    "lint": "eslint . --cache --cache-location 'node_modules/.cache/.eslintcache' --max-warnings 0",
    "start": "remix-serve build",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@clerk/remix": "3.1.18",
    "@remix-run/react": "2.6.0",
    "@remix-run/serve": "2.6.0",
    "@sentry/integrations": "7.101.1",
    "@sentry/profiling-node": "7.101.1",
    "@sentry/remix": "7.101.1",
    "@technifit/ui": "1.0.0",
    "@vercel/analytics": "1.2.2",
    "@vercel/remix": "2.6.0",
    "@vercel/speed-insights": "1.0.10",
    "isbot": "5.1.0",
    "lodash": "4.17.21",
    "lucide-react": "0.334.0",
    "nanoid": "5.0.5",
    "pretty-cache-header": "1.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remix-hook-form": "4.1.0",
    "remix-params-helper": "0.5.1",
    "remix-routes": "1.7.2",
    "remix-themes": "1.3.1",
    "remix-utils": "7.5.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@remix-run/dev": "2.6.0",
    "@technifit/eslint-config": "1.0.0",
    "@technifit/tailwind-config": "1.0.0",
    "@technifit/tsconfig": "1.0.0",
    "@types/cookie": "0.6.0",
    "@types/lodash": "4.14.202",
    "@types/react": "18.2.57",
    "@types/react-dom": "18.2.19",
    "cookie": "0.6.0",
    "eslint": "8.56.0",
    "npm-run-all2": "6.1.2",
    "remix-flat-routes": "0.6.4",
    "tailwindcss": "3.4.1",
    "tailwindcss-animate": "1.0.7",
    "typescript": "5.3.3",
    "typescript-remix-routes-plugin": "1.0.1"
  },
  "engines": {
    "node": ">=20.11.1"
  }
}
