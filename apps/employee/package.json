{
  "name": "@technifit/employee",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "clean": "rm -rf .turbo node_modules coverage playwright-report test-results .cache build types/remix-routes.d.ts",
    "dev": "remix vite:dev",
    "docker:build": "cd ../.. && docker build -f apps/employee/Dockerfile -t technifit-employee .",
    "docker:run": "cd ../.. && docker run --rm -d -p 8080:8080 --env-file .env technifit-employee",
    "format:fix": "prettier . --write --cache --cache-location='node_modules/.cache/.prettiercache' --log-level=warn",
    "format": "prettier . --check --cache --cache-location='node_modules/.cache/.prettiercache'",
    "lint:fix": "eslint . --fix --cache --cache-location 'node_modules/.cache/.eslintcache' --max-warnings 0",
    "lint": "eslint . --cache --cache-location 'node_modules/.cache/.eslintcache' --max-warnings 0",
    "start:docker": "run-s docker:*",
    "start": "node ./build/server/index.js",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest",
    "test": "vitest --run",
    "test:integration": "playwright test",
    "test:integration-ui": "playwright test --ui",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hono/node-server": "1.9.1",
    "@remix-run/express": "2.8.1",
    "@remix-run/node": "2.8.1",
    "@remix-run/react": "2.8.1",
    "@technifit/events": "*",
    "@technifit/hooks": "*",
    "@technifit/theme": "*",
    "@technifit/ui": "*",
    "colortranslator": "4.1.0",
    "dotenv": "16.4.5",
    "hono": "4.2.1",
    "i18next": "23.10.1",
    "i18next-browser-languagedetector": "7.2.1",
    "i18next-fs-backend": "2.3.1",
    "i18next-http-backend": "2.5.0",
    "isbot": "5.1.4",
    "lodash": "4.17.21",
    "nanoid": "5.0.6",
    "pretty-cache-header": "1.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "14.1.0",
    "remix-hono": "0.0.16",
    "remix-i18next": "6.0.1",
    "remix-routes": "1.7.2",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@hono/vite-dev-server": "0.10.0",
    "@playwright/test": "1.42.1",
    "@remix-run/dev": "2.8.1",
    "@technifit/eslint-config": "*",
    "@technifit/tailwind-config": "*",
    "@technifit/tsconfig": "*",
    "@types/lodash": "4.17.0",
    "@types/node": "20.12.3",
    "@types/react": "18.2.74",
    "@types/react-dom": "18.2.23",
    "@vitest/coverage-v8": "1.4.0",
    "autoprefixer": "10.4.19",
    "cross-env": "7.0.3",
    "dotenv-cli": "7.4.1",
    "esbuild": "0.20.2",
    "npm-run-all2": "6.1.2",
    "postcss": "8.4.38",
    "remix-development-tools": "4.1.4",
    "remix-flat-routes": "0.6.4",
    "tailwindcss": "3.4.3",
    "tsx": "4.7.1",
    "typescript": "5.4.3",
    "typescript-remix-routes-plugin": "1.0.1",
    "vite": "5.2.7",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "1.4.0"
  },
  "engines": {
    "node": ">=20.12.0"
  }
}
